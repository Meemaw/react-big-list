{
  "name": "react-big-list",
  "version": "1.0.0",
  "description": "Smart wrapper component for your big collections.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Meemaw <ematej.snuderl@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "jest",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "dependencies": {
    "lodash.orderby": "^4.6.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.4",
    "@storybook/addon-info": "^3.3.4",
    "@storybook/addon-knobs": "^3.3.4",
    "@storybook/addon-links": "^3.3.4",
    "@storybook/addon-notes": "^3.3.4",
    "@storybook/addon-options": "^3.3.4",
    "@storybook/react": "^3.3.4",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.0",
    "@types/react": "^16.4.8",
    "@types/react-dom": "^16.0.7",
    "@types/react-test-renderer": "^16.0.2",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-info": "^3.2.1",
    "@types/storybook__addon-knobs": "^3.2.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__addon-notes": "^3.3.0",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.6",
    "awesome-typescript-loader": "^4.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.5.0",
    "moment": "2.18.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "semantic-ui-react": "^0.82.2",
    "sinon": "^6.1.5",
    "size-limit": "^0.19.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "setupFiles": [
      "./tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/tests/.*.(spec).(tsx)$"
  }
}
